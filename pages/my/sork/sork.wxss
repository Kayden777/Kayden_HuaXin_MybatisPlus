/* // page设置当前窗口背景色 */

page {
  background-color: #f5f5f5;
}

/* // 定义.container设置容器样式 */

.container {
  padding: 10px 20px;
  align-content: center;
}

/* // 定义.text-content设置文本样式 */

.text-content {
  padding: 20px;
  width: 100%;
  align-content: center;
  border-bottom: 1rpx solid #f5f5f5;
  background-color: #fff;
}

.namestyle {
  width: 190rpx;
  height: 55rpx;
  overflow: hidden;
}

.userURLStyle{
  margin-left: 14rpx;
}
